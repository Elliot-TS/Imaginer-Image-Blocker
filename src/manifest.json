{
  "manifest_version": 2,
  "name": "Imaginer Image Blocker",
  "description": "A Fast and Powerful Image Blocker Extension",
  "version": "1.0",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "background":
  {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activateTab",
    "storage",
  ],
}
